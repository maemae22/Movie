<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.movie.mapper.AdminMapper">
    <select id="selectAdmin" parameterType="com.example.movie.admin.dto.AdminDTO" resultType="com.example.movie.admin.dto.AdminDTO">
        SELECT
            id
             ,password
        FROM
            admin
        WHERE
            id = #{id}
          AND
            password = #{password}
    </select>

</mapper>